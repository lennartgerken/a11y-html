{
    "name": "a11y-html",
    "version": "1.1.0",
    "license": "MIT",
    "author": "Lennart Gerken",
    "description": "Takes axe-core results and creates an html report.",
    "keywords": [
        "a11y",
        "accessibility",
        "axe",
        "html",
        "report"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/lennartgerken/a11y-html.git"
    },
    "bugs": {
        "url": "https://github.com/lennartgerken/a11y-html/issues"
    },
    "type": "module",
    "types": "./createReport/dist/index.d.ts",
    "exports": {
        "types": "./createReport/dist/index.d.ts",
        "import": "./createReport/dist/a11y-html.js",
        "require": "./createReport/dist/a11y-html.cjs"
    },
    "module": "./createReport/dist/a11y-html.js",
    "main": "./createReport/dist/a11y-html.cjs",
    "files": [
        "createReport/dist",
        "LICENSES-THIRD-PARTY.txt"
    ],
    "scripts": {
        "build": "npm run build-vue && npm run build-create-report",
        "dev": "vite",
        "test": "playwright test",
        "pretty": "prettier . --write",
        "pretty-check": "prettier . --check",
        "lint": "eslint",
        "build-vue": "vue-tsc --build && vite build",
        "preview-vue": "vite preview",
        "watch-vue": "vue-tsc --watch",
        "build-create-report": "vite build ./createReport && tsc -p ./createReport  --emitDeclarationOnly true"
    },
    "peerDependencies": {
        "axe-core": "^4.0.0"
    },
    "devDependencies": {
        "@axe-core/playwright": "^4.10.2",
        "@playwright/test": "^1.55.0",
        "@tailwindcss/vite": "^4.1.12",
        "@types/node": "^24.3.0",
        "@vitejs/plugin-vue": "^6.0.1",
        "@vue/tsconfig": "^0.8.1",
        "eslint": "^9.34.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-vue": "^10.4.0",
        "globals": "^16.3.0",
        "prettier": "3.6.2",
        "tailwindcss": "^4.1.12",
        "typescript": "^5.9.2",
        "typescript-eslint": "^8.40.0",
        "vite": "^7.1.3",
        "vite-plugin-singlefile": "^2.3.0",
        "vite-plugin-vue-devtools": "^8.0.0",
        "vue": "^3.5.19",
        "vue-tsc": "^3.0.6"
    }
}
